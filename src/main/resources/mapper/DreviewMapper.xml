<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxa.mapper.DreviewMapper">

    <resultMap id="dreviewResultMap" type="dreview">
        <id property="id" column="id"></id>
        <result property="mperson" column="mperson"></result>
        <result property="status" column="status"></result>
        <result property="reviewer" column="reviewer"></result>
    </resultMap>

    <select id="queryDreivews" resultMap="dreviewResultMap">
        SELECT id,mperson,status,reviewer FROM d_review
    </select>
</mapper>