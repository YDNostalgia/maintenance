<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxa.mapper.DwarehouseMapper">

    <resultMap id="dwarehouseResultMap" type="dwarehouse">
        <id property="id" column="id"></id>
        <result property="code" column="code"></result>
        <result property="bill" column="bill"></result>
        <result property="time" column="time"></result>
        <association property="dreview" javaType="com.gxa.entity.Dreview">
            <id property="id" column="id"></id>
            <result property="mperson" column="mperson"></result>
            <result property="status" column="status"></result>
            <result property="reviewer" column="reviewer"></result>
        </association>
    </resultMap>

    <select id="queryDwarehouse" resultType="com.gxa.entity.Dwarehouse">
        SELECT dw.time,dw.code,dw.bill,dr.mperson,dr.status,dr.reviewer
        FROM d_warehouse dw,d_review dr
        WHERE dw.reviewId=dr.id
    </select>
</mapper>