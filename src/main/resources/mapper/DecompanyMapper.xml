<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxa.mapper.DecompanyMapper">

    <resultMap id="decompanyResultMap" type="decompany">
        <id property="id" column="id"></id>
        <result property="company" column="company"></result>
    </resultMap>
    <select id="queryCompany" resultMap="decompanyResultMap">
        SELECT * FROM d_company
    </select>
</mapper>