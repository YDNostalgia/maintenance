<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxa.mapper.DpickingMapper">

    <resultMap id="DpickingResultMap" type="dpicking">
        <id property="id" column="id"></id>
        <result property="code" column="code"></result>
        <result property="department" column="department"></result>
        <result property="time" column="time"></result>
        <association property="dreview" javaType="com.gxa.entity.Dreview">
            <id property="id" column="id"></id>
            <result property="mperson" column="mperson"></result>
            <result property="status" column="status"></result>
            <result property="reviewer" column="reviewer"></result>
        </association>

    </resultMap>
    <select id="queryPickings" resultType="com.gxa.entity.Dpicking">
        SELECT dp.time,dp.code,dp.department,dr.mperson,dr.status,dr.reviewer
        FROM d_review dr,d_picking dp
        WHERE dp.reviewId=dr.id
    </select>
</mapper>